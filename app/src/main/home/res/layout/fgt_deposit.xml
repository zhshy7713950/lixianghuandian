<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res-auto"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="@color/bg_gray"
		android:orientation="vertical">

	<include layout="@layout/layout_topbar" />

	<ScrollView
			android:id="@+id/scroll_account_deposit"
			android:layout_width="match_parent"
			android:layout_height="match_parent">

		<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="vertical"
			android:background="#2E3440">

			<androidx.constraintlayout.widget.ConstraintLayout
					android:layout_width="match_parent"
					android:layout_height="150dp"
					android:layout_marginTop="12dp"
				android:layout_marginLeft="12dp"
				android:layout_marginRight="12dp"
					android:background="@drawable/rectangle_gray_bg">
				<ImageView
					android:id="@+id/iv_battery_account_deposit"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_margin="10dp"
					app:layout_constraintLeft_toLeftOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					android:src="@mipmap/battery_five"
					android:layout_marginLeft="16dp"
					android:layout_marginTop="20dp"/>
				<TextView
					android:id="@+id/tv2"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="电池编号"
					android:layout_marginLeft="136dp"
					android:layout_marginTop="16dp"
					app:layout_constraintLeft_toLeftOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					android:textColor="#ffb2c1ce"
					android:textSize="12sp"
					/>
				<TextView
					android:id="@+id/tv_battery_num_account_deposit"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					tools:text="10000222"
					android:layout_marginLeft="136dp"
					android:layout_marginTop="37dp"
					app:layout_constraintLeft_toLeftOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					android:textColor="#ffffffff"
					android:textSize="16sp"
					/>

				<TextView
					android:id="@+id/tv1"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="电池型号"
					android:textColor="#ffb2c1ce"
					android:textSize="12sp"
					android:layout_marginRight="41dp"
					app:layout_constraintRight_toRightOf="parent"
					app:layout_constraintTop_toTopOf="parent"
					android:layout_marginTop="16dp"
					/>
				<TextView
					android:id="@+id/tv_battery_model_account_deposit"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="48伏50安"
					android:textColor="#ffffffff"
					app:layout_constraintLeft_toLeftOf="@id/tv1"
					app:layout_constraintTop_toBottomOf="@id/tv1"
					android:layout_marginTop="4dp"
					android:textSize="16sp"
					/>
				<View
					android:layout_width="0.5dp"
					android:layout_height="44dp"
					android:background="@drawable/shape_line"
					android:layout_marginLeft="53dp"
					android:layout_marginTop="16dp"
					android:layout_marginRight="24dp"
					app:layout_constraintTop_toTopOf="parent"
					app:layout_constraintRight_toLeftOf="@id/tv1"
					app:layout_constraintLeft_toRightOf="@id/tv2"/>
				<View
					android:layout_width="match_parent"
					android:layout_height="0.5dp"
					android:background="@drawable/shape_line"
					app:layout_constraintTop_toTopOf="parent"
					android:layout_marginTop="75dp"/>
				<TextView
					android:id="@+id/tv_money_account_deposit"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					tools:text="¥ 200.00"
					android:textColor="#ffffffff"
					android:textSize="20sp"
					app:layout_constraintLeft_toRightOf="@id/tv4"
					android:layout_marginBottom="24dp"
					app:layout_constraintBottom_toBottomOf="parent"
					android:layout_marginLeft="14dp"
					/>
				<TextView
					android:id="@+id/tv4"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="需支付押金："
					android:textColor="#ffb2c1ce"
					android:textSize="12sp"
					app:layout_constraintLeft_toLeftOf="parent"
					app:layout_constraintBottom_toBottomOf="parent"
					android:layout_marginLeft="75dp"
					android:layout_marginBottom="26dp"
					/>
			</androidx.constraintlayout.widget.ConstraintLayout>

			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="支付选项"
				android:layout_marginLeft="12dp"
				android:layout_marginTop="24dp"
				android:textColor="#ffffffff"
				android:textSize="15sp"
				/>
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="vertical"
				android:layout_marginLeft="12dp"
				android:layout_marginTop="12dp"
				android:background="@drawable/rectangle_gray_bg"
				android:layout_marginRight="12dp">
				<androidx.constraintlayout.widget.ConstraintLayout
					android:layout_width="match_parent"
					android:layout_height="48dp">
					<TextView
						android:id="@+id/tv3"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="经销商编号"
						android:textColor="#ff929fab"
						app:layout_constraintLeft_toLeftOf="parent"
						android:textSize="14sp"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						android:layout_marginLeft="14dp"
						/>
					<EditText
						android:id="@+id/et_agnet_name_deposit_code"
						android:layout_width="120dp"
						android:layout_height="wrap_content"
						android:background="@null"
						android:hint="请输入经销商编号"
						tools:text="784533"
						android:textColor="#ff29ebb6"
						android:textSize="14sp"
						android:gravity="right"
						android:inputType="textWebEditText"
						app:layout_constraintRight_toRightOf="parent"
						android:layout_marginRight="33dp"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
					 />
					<View
						android:layout_width="match_parent"
						android:layout_height="0.5dp"
						android:background="@drawable/shape_line"
						app:layout_constraintBottom_toBottomOf="parent"/>
				</androidx.constraintlayout.widget.ConstraintLayout>
				<androidx.constraintlayout.widget.ConstraintLayout
					android:layout_width="match_parent"
					android:layout_height="48dp">
					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="经销商名称"
						android:textColor="#ff929fab"
						app:layout_constraintLeft_toLeftOf="parent"
						android:textSize="14sp"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						android:layout_marginLeft="14dp"
						/>
					<TextView
						android:id="@+id/et_agnet_name_deposit"
						android:layout_width="120dp"
						android:layout_height="wrap_content"
						tools:text="784533"
						android:textColor="#ff29ebb6"
						android:textSize="14sp"
						android:gravity="right"
						android:inputType="textWebEditText"
						app:layout_constraintRight_toRightOf="parent"
						android:layout_marginRight="33dp"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						/>
					<View
						android:layout_width="match_parent"
						android:layout_height="0.5dp"
						android:background="@drawable/shape_line"
						app:layout_constraintBottom_toBottomOf="parent"/>
				</androidx.constraintlayout.widget.ConstraintLayout>
				<androidx.constraintlayout.widget.ConstraintLayout
					android:layout_width="match_parent"
					android:layout_height="48dp">
					<TextView
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="是否整租车架"
						android:textColor="#ff929fab"
						app:layout_constraintLeft_toLeftOf="parent"
						android:textSize="14sp"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						android:layout_marginLeft="14dp"
						/>
					<TextView
						android:id="@+id/tv_account_deposit"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="请选择"
						android:textColor="#ff828f9b"
						android:textSize="14sp"
						android:drawableRight="@drawable/arrow_right"
						app:layout_constraintRight_toRightOf="parent"
						android:layout_marginRight="14dp"
						android:drawablePadding="13dp"
						app:layout_constraintTop_toTopOf="parent"
						app:layout_constraintBottom_toBottomOf="parent"
						/>
					<View
						android:layout_width="match_parent"
						android:layout_height="0.5dp"
						android:background="@drawable/shape_line"
						app:layout_constraintBottom_toBottomOf="parent"/>
				</androidx.constraintlayout.widget.ConstraintLayout>
				<androidx.constraintlayout.widget.ConstraintLayout
					android:layout_width="match_parent"
					android:layout_height="103dp">
					<TextView
						android:id="@+id/tv5"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="组合方式"
						android:textColor="#ff929fab"
						app:layout_constraintLeft_toLeftOf="parent"
						android:textSize="14sp"
						app:layout_constraintTop_toTopOf="parent"
						android:layout_marginLeft="14dp"
						android:layout_marginTop="14dp"
						/>

					<TextView
						android:id="@+id/tv_select_package_deposit"
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_marginRight="14dp"
						android:layout_marginLeft="14dp"
						android:drawableRight="@drawable/arrow_right"
						android:drawablePadding="13dp"
						android:maxLines="2"
						android:text="请选择"
						android:textColor="#ff828f9b"
						android:textSize="14sp"
						app:layout_constraintBottom_toBottomOf="parent"
						app:layout_constraintRight_toRightOf="parent"
						app:layout_constraintLeft_toLeftOf="parent"
						app:layout_constraintTop_toBottomOf="@id/tv5" />
					<View
						android:layout_width="match_parent"
						android:layout_height="0.5dp"
						android:background="@drawable/shape_line"
						app:layout_constraintBottom_toBottomOf="parent"/>
				</androidx.constraintlayout.widget.ConstraintLayout>
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="请选择支付方式"
					android:textColor="#ff929fab"
					android:textSize="14sp"
					android:layout_marginTop="14dp"
					android:layout_marginLeft="14dp"
					/>
				<RadioGroup
					android:id="@+id/rgDeposit"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="14dp"
					android:layout_marginLeft="14dp">
					<RadioButton
						android:id="@+id/rbAlipay"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layoutDirection="rtl"
						android:text="支付宝"
						android:textColor="@color/white"
						android:textSize="14sp"
						android:layout_marginLeft="14dp"
						android:layout_marginRight="14dp"
						android:drawablePadding="12dp"
						android:layout_marginTop="16dp"
						android:drawableLeft="@mipmap/ic_withdrawal_alipay"
						android:drawableRight="@drawable/radio_select"
						android:button="@null">
					</RadioButton>
					<RadioButton
						android:id="@+id/rbWx"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layoutDirection="rtl"
						android:text="微信"
						android:textColor="@color/white"
						android:textSize="14sp"
						android:layout_marginLeft="14dp"
						android:layout_marginRight="14dp"
						android:drawablePadding="12dp"
						android:layout_marginTop="14dp"
						android:drawableLeft="@mipmap/ic_withdrawal_wx"
						android:drawableRight="@drawable/radio_select"
						android:button="@null">
					</RadioButton>
					<RadioButton
						android:id="@+id/rbOffline"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layoutDirection="rtl"
						android:text="线下支付"
						android:textColor="@color/white"
						android:textSize="14sp"
						android:layout_marginLeft="14dp"
						android:layout_marginRight="14dp"
						android:drawablePadding="12dp"
						android:layout_marginTop="14dp"
						android:layout_marginBottom="20dp"
						android:drawableLeft="@mipmap/ic_pay_offline"
						android:drawableRight="@drawable/radio_select"
						android:button="@null">
					</RadioButton>
				</RadioGroup>


			</LinearLayout>
			<FrameLayout
				android:layout_width="match_parent"
				android:layout_height="40dp"
				android:layout_marginTop="16dp">

				<LinearLayout
					android:layout_width="wrap_content"
					android:layout_height="20dp"
					android:layout_gravity="left"
					android:layout_marginLeft="12dp"
					android:orientation="horizontal">

					<ImageView
						android:id="@+id/iv_check"
						android:layout_width="20dp"
						android:layout_height="20dp"
						android:layout_marginRight="5dp"
						android:scaleType="centerInside"
						android:src="@mipmap/ic_radio_unselect" />

					<TextView
						android:layout_width="wrap_content"
						android:layout_height="20dp"
						android:gravity="center_vertical"
						android:text="我已仔细阅读并同意"
						android:textColor="#B8C7D7"
						android:layout_marginLeft="10dp"
						android:textSize="12sp" />

					<TextView
						android:id="@+id/tv_view_rant_protocol_account_ya"
						android:layout_width="wrap_content"
						android:layout_height="20dp"
						android:gravity="center_vertical"
						android:text="《租赁协议》"
						android:textColor="#29EBB6"
						android:textSize="12sp" />
				</LinearLayout>
			</FrameLayout>

			<androidx.constraintlayout.widget.ConstraintLayout
				android:layout_width="match_parent"
				android:layout_height="50dp"
				android:layout_marginLeft="12dp"
				android:layout_marginRight="12dp"
				android:layout_marginBottom="20dp"
				android:background="@drawable/rectangle_gray_bg">
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:text="合计："
					android:textColor="#ffffffff"
					android:textSize="14sp"
					app:layout_constraintLeft_toLeftOf="parent"
					android:gravity="center_vertical"
					android:layout_marginLeft="14dp"
					/>
				<TextView
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:text="¥200.00"
					android:textColor="#ff29ebb6"
					android:textSize="14sp"
					app:layout_constraintLeft_toLeftOf="parent"
					android:layout_marginLeft="60dp"
					android:gravity="center_vertical"
					/>
				<TextView
					android:id="@+id/btn_pay_now_account_deposit"
					android:layout_width="100dp"
					android:layout_height="match_parent"
					android:text="立即支付"
					android:textColor="#ff272934"
					android:textSize="15sp"
					android:gravity="center"
					app:layout_constraintRight_toRightOf="parent"
					android:background="@drawable/rectangle_pay_bg"
					/>
			</androidx.constraintlayout.widget.ConstraintLayout>

