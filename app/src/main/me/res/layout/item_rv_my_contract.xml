<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:tools="http://schemas.android.com/tools"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:background="@color/bg_gray"
		android:orientation="vertical"
		android:paddingLeft="10dp"
		android:paddingRight="10dp"
		android:paddingBottom="5dp">


	<com.qmuiteam.qmui.widget.roundwidget.QMUIRoundFrameLayout xmlns:app="http://schemas.android.com/apk/res-auto"
			android:layout_width="match_parent"
			android:layout_height="100dp"
			app:qmui_backgroundColor="@color/white"
			app:qmui_radius="5dp">

		<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="horizontal">


			<ImageView
					android:id="@+id/iv"
					android:layout_width="60dp"
					android:layout_height="80dp"
					android:layout_gravity="center"
					android:layout_marginLeft="10dp"
					android:scaleType="fitXY"
					android:src="@drawable/icon_battery_placeholder" />


			<LinearLayout
					android:layout_width="0dp"
					android:layout_height="match_parent"
					android:layout_weight="1"
					android:orientation="vertical"
					android:padding="10dp">

				<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="0dp"
						android:layout_weight="1"
						android:orientation="horizontal">

					<TextView
							android:id="@+id/tv_num"
							android:layout_width="0dp"
							android:layout_height="match_parent"
							android:layout_weight="1"
							android:gravity="center_vertical"
							android:paddingLeft="10dp"
							android:textSize="10sp"
							tools:text="编号：1518" />


					<TextView
							android:id="@+id/tv_model"
							android:layout_width="0dp"
							android:layout_height="match_parent"
							android:layout_weight="1"
							android:gravity="center"
							android:textSize="10sp"
							tools:text="型号：60V    40AH" />


					<TextView
							android:id="@+id/tv_rent_long"
							android:layout_width="0dp"
							android:layout_height="match_parent"
							android:layout_weight="1"
							android:gravity="center_vertical|right"
							android:textSize="10sp"
							tools:text="租期：9个月" />
				</LinearLayout>

				<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="0dp"
						android:layout_weight="1"
						android:orientation="horizontal">

					<TextView
							android:id="@+id/tv_rent_start_time"
							android:layout_width="0dp"
							android:layout_height="match_parent"
							android:layout_weight="1"
							android:gravity="center_vertical"
							android:paddingLeft="10dp"
							android:textSize="10sp"
							tools:text="租用日期：2019-11-21" />


					<TextView
							android:id="@+id/tv_rent_end_time"
							android:layout_width="0dp"
							android:layout_height="match_parent"
							android:layout_weight="1"
							android:gravity="center_vertical|right"
							android:textSize="10sp"
							tools:text="结束日期：2020-11-21" />
				</LinearLayout>


				<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="0dp"
						android:layout_weight="1"
						android:orientation="horizontal">

					<com.qmuiteam.qmui.widget.roundwidget.QMUIRoundFrameLayout
							android:id="@+id/qrf_sign"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_gravity="center"
							android:layout_marginLeft="10dp"
							android:paddingLeft="10dp"
							android:paddingTop="3dp"
							android:paddingRight="10dp"
							android:paddingBottom="3dp"
							app:qmui_backgroundColor="#FFA5A5"
							app:qmui_radius="5dp">

						<TextView
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="补签合同"
								android:textColor="@color/white"
								android:textSize="10sp" />
					</com.qmuiteam.qmui.widget.roundwidget.QMUIRoundFrameLayout>


					<com.qmuiteam.qmui.widget.roundwidget.QMUIRoundFrameLayout
							android:id="@+id/qrf_return"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_gravity="center"
							android:layout_marginLeft="15dp"
							android:paddingLeft="10dp"
							android:paddingTop="3dp"
							android:paddingRight="10dp"
							android:paddingBottom="3dp"
							app:qmui_backgroundColor="#2DCB52"
							app:qmui_radius="5dp">

						<TextView
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="退还押金"
								android:textColor="@color/white"
								android:textSize="10sp" />
					</com.qmuiteam.qmui.widget.roundwidget.QMUIRoundFrameLayout>
				</LinearLayout>


			</LinearLayout>

			<ImageView
					android:id="@+id/iv_enter"
					android:layout_width="20dp"
					android:layout_height="20dp"
					android:layout_gravity="center"
					android:layout_marginRight="10dp"
					android:scaleType="centerInside"
					android:src="@drawable/arrow_right" />
		</LinearLayout>

	</com.qmuiteam.qmui.widget.roundwidget.QMUIRoundFrameLayout>
</FrameLayout>